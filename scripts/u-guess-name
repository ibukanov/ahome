#!/bin/bash

# Guess based on MAC address
link_text="$(ip link show)"

test_mac() {
    mac="$1"
    guessed_name="$2"
    test "x$link_text" != "x${link_text/$mac}" && { echo $guessed_name; exit 0; }      
}

test_mac 20:02:af:e2:89:9b chromebook
test_mac 00:24:d7:e0:30:cc thinkpad

echo unknown


