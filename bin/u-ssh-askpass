#!/bin/sh

# shellcheck shell=dash
# shellcheck enable=all
# shellcheck disable=SC2250,SC2034

set -e -u

if test -x /usr/bin/lxqt-openssh-askpass; then
    exec /usr/bin/lxqt-openssh-askpass "$@" 2>/dev/null
fi
if test -x /usr/lib/openssh/gnome-ssh-askpass; then
    exec /usr/lib/openssh/gnome-ssh-askpass "$@"
fi
s="$(command -v ssh-askpass)"
if test "$s" && test -x "$s"; then
    exec "$s" "$@"
fi

printf '%s: Error - suitable ssh-askpass program\n' "$0" >&2
exit 1
